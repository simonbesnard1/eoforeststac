   "source": [
    "# Align Two Collections for Combined Analysis\n",
    "This example aligns EFDA and CCI Biomass for a region of interest."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "from eoforeststac.providers.zarr import ZarrProvider\n",
    "from eoforeststac.providers.subset import subset\n",
    "from eoforeststac.analysis.align import DatasetAligner\n",
    "import geopandas as gpd\n",
    "\n",
    "provider = ZarrProvider(\n",
    "    catalog_url=\"s3://dog.atlaseo-glm.eo-gridded-data/collections/catalog.json\",\n",
    "    endpoint_url=\"https://s3.gfz-potsdam.de\",\n",
    "    anon=True,\n",
    ")\n",
    "\n",
    "# Open datasets\n",
    "ds_biomass = provider.open_dataset(\"CCI_BIOMASS\", \"6.0\")\n",
    "ds_efda = provider.open_dataset(\"EFDA\", \"2.1\")\n",
    "\n",
    "# Define a region of interest\n",
    "roi = gpd.read_file(\"geojson/DE-Hai.geojson\")\n",
    "geometry = roi.to_crs(\"EPSG:4326\").geometry.union_all()\n",
    "\n",
    "# Subset each\n",
    "ds_b_b = subset(ds_biomass, geometry=geometry, lon=\"longitude\", lat=\"latitude\")\n",
    "ds_e     = subset(ds_efda,    geometry=geometry, lon=\"longitude\", lat=\"latitude\")\n",
    "\n",
    "# Align\n",
    "aligner = DatasetAligner(\n",
    "    target=\"CCI_BIOMASS\",\n",
    "    resampling={\n",
    "        \"CCI_BIOMASS\": \"average\",\n",
    "        \"EFDA\": \"nearest\",\n",
    "    },\n",
    ")\n",
    "aligned = aligner.align({\n",
    "    \"CCI_BIOMASS\": ds_b_b,\n",
    "    \"EFDA\": ds_e,\n",
    "})\n",
    "\n",
    "print(aligned)\n",
    "\n",
    "# Plot combined view\n",
    "aligned.aboveground_biomass.plot(cmap=\"viridis\")\n",
    "aligned.efda_disturbance.plot(cmap=\"inferno\")"
   ]
  }
